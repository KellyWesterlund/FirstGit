#Grading
grade = input('Grade?')
try:
    grade = float(grade)
    if grade > 1.0:
        print('error')
    elif grade < 0.0:
        print('error')
    elif grade >= 0.9:
        print('A')
    elif grade >= 0.8:
        print('B')
    elif grade >= 0.7:
        print('C')
    elif grade >= 0.6:
        print('D')
    else:
        print('F')
except:
    print('Please enter grade')
